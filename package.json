{
  "name": "covid19stat",
  "version": "1.0.0",
  "description": "world covid19 statistics",
  "main": "index.js",
  "engines": {
    "node": "8.1.1",
    "npm": "5.0.3"
  },
  "scripts": {
    "start": "npm run start --prefix Server",
    "back-end": "npm run dev --prefix Server",
    "front-end": "npm run start --prefix Covid19stat",
    "start-app": "concurrently \" npm run front-end \" npm run back-end",
    "start-dev": "concurrently \" npm run front-end \" npm run back-end",
    "heroku-postbuild":"NPM_CONFIG_PRODUCTION=false npm install --prefix Server",
    "heroku-postbuild":"NPM_CONFIG_PRODUCTION=false npm install --prefix Covid19stat",
    "heroku-postbuild":"NPM_CONFIG_PRODUCTION=false npm run build --prefix Covid19stat"
  },
  "author": "Joshua Evuetapha",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/Joshuajee/Covid19"
  },
  "keywords": [
    "covid"
  ],
  "dependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.6"
  }
}
